{
  "name": "Tilted",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A prank extension that skews page content slightly.",
  "homepage_url": "https://github.com/GaryGeorgeu/tilted-chrome-extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}